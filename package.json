{
  "name": "git-to-daily",
  "version": "0.1.0",
  "description": "CLI tool that generates Obsidian daily logs from git commits",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "git-to-daily": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "prepare": "git config core.hooksPath .githooks",
    "hooks:install": "git config core.hooksPath .githooks && echo Hooks installed! Set GIT_TO_DAILY_VAULT environment variable to enable."
  },
  "keywords": [
    "git",
    "obsidian",
    "daily-log",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
